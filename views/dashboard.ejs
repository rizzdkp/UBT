    <script src="/socket.io/socket.io.js"></script>
    <script src="/toast.js"></script>
    <script>
      const socket = io();
      
      socket.on('status_updated', function(data) {
        showToast(`Protokol ${data.code} diperbarui menjadi ${data.newStatus}`, 'success');
        updateTableRow(data.protocol);
        setTimeout(() => {
          window.location.reload();
        }, 1500);
      });

      if ('serviceWorker' in navigator) {
        navigator.serviceWorker.getRegistrations().then(function(registrations) {
          for(let registration of registrations) {
            registration.unregister();
          }
        });
        caches.keys().then(function(names) {
          for (let name of names) {
            caches.delete(name);
          }
        });
      }
      
      function updateTableRow(protocol) {
        const rows = document.querySelectorAll('tbody tr');
        rows.forEach(row => {
          const codeCell = row.querySelector('td:nth-child(2)');
          if (codeCell && codeCell.textContent.includes(protocol.code)) {
            const statusCell = row.querySelector('td:nth-child(5)');
            const statusBadge = statusCell.querySelector('.status-badge');
            statusBadge.className = `status-badge status-${protocol.status}`;
            const statusText = {
              'created': 'Dibuat',
              'delivered': 'Terkirim',
              'terpakai': 'Terpakai'
            };
            statusBadge.textContent = statusText[protocol.status] || protocol.status;
          }
        });
      }
      
      function showToast(message, type = 'info') {
        let toast = document.getElementById('dashboard-toast');
        if (!toast) {
          toast = document.createElement('div');
          toast.id = 'dashboard-toast';
          toast.className = 'dashboard-toast';
          document.body.appendChild(toast);
        }
        
        toast.textContent = message;
        toast.className = `dashboard-toast dashboard-toast-${type} show`;
        
        setTimeout(() => {
          toast.classList.remove('show');
        }, 3000);
      }

      // Patient detail modal implementation
      const _patientModal = document.createElement('div');
      _patientModal.id = 'patientModal';
      _patientModal.className = 'patient-modal-backdrop';
      _patientModal.innerHTML = `
        <div class="patient-modal" role="dialog" aria-modal="true" aria-labelledby="patientModalTitle">
          <div class="patient-modal__header">
            <div id="patientModalTitle" class="patient-modal__title">ü©∫ Data Pasien OBGYN</div>
            <div style="display: flex; gap: 10px; align-items: center;">
              <button id="patientEdit" class="patient-modal__edit-toggle" aria-label="Edit Data" style="display: none;">
                <span>‚úèÔ∏è</span> Edit
              </button>
              <button id="patientClose" class="patient-modal__close" aria-label="Tutup">‚úï</button>
            </div>
          </div>
          <div id="patientBody" class="patient-modal__body">
            <div id="p_name" class="patient-modal__name">-</div>
            <p id="p_code" class="patient-modal__code">Kode Protokol: -</p>
            <hr class="patient-modal__divider"/>
            <div id="p_grid" class="patient-modal__grid"></div>
            <div class="patient-modal__gpa">
              <div class="patient-modal__gpa-title">GPA (Gravida - Para - Abortus)</div>
              <div id="p_gpa" class="patient-modal__gpa-grid"></div>
            </div>
            <div id="p_footer" class="patient-modal__footer"></div>
            <div id="p_actions" class="patient-modal__footer" style="display: none; margin-top: 20px;">
              <button id="patientSave" class="patient-modal__save-btn">üíæ Simpan Perubahan</button>
              <button id="patientCancel" class="patient-modal__cancel-btn">‚ùå Batal</button>
            </div>
          </div>
        </div>`;
      document.body.appendChild(_patientModal);

      let _editMode = false;
      let _currentPatientData = null;

      function closePatientModal(){ 
        _patientModal.style.display = 'none'; 
        _editMode = false;
        _currentPatientData = null;
      }
      
      document.addEventListener('click', (e)=>{
        if (e.target && (e.target.id === 'patientClose' || e.target.id === 'patientModal')) closePatientModal();
      });

      function _badge(label, val, fieldName){
        if (_editMode && fieldName) {
          return `
            <div class="patient-badge">
              <div class="patient-badge__label">${label}</div>
              <input type="text" class="patient-badge__input" data-field="${fieldName}" value="${val || ''}" />
            </div>`;
        }
        return `
          <div class="patient-badge">
            <div class="patient-badge__label">${label}</div>
            <div class="patient-badge__value">${val || '-'}</div>
          </div>`;
      }
      
      function toggleEditMode() {
        _editMode = !_editMode;
        if (_currentPatientData) {
          renderPatientData(_currentPatientData);
        }
      }

      function renderPatientData(data) {
        _currentPatientData = data;
        const editBtn = document.getElementById('patientEdit');
        const actionsDiv = document.getElementById('p_actions');
        
        if (_editMode) {
          document.getElementById('p_name').innerHTML = `<input type="text" class="patient-badge__input" data-field="patient_name" value="${data.patient_name || ''}" style="font-size: 20px; font-weight: 800; color: #e91e63;" />`;
          editBtn.textContent = 'üëÅÔ∏è Lihat';
          actionsDiv.style.display = 'flex';
        } else {
          document.getElementById('p_name').textContent = data.patient_name || '-';
          editBtn.innerHTML = '<span>‚úèÔ∏è</span> Edit';
          actionsDiv.style.display = 'none';
        }
        
        document.getElementById('p_code').textContent = `Kode Protokol: ${data.protocol_code || '-'}`;
        document.getElementById('p_grid').innerHTML = [
          _badge('Nama Faskes', data.faskes_name, 'faskes_name'),
          _badge('Pekerjaan', data.pekerjaan, 'pekerjaan'),
          _badge('Status Pekerjaan', data.status_pekerjaan, 'status_pekerjaan'),
          _badge('Status Pernikahan', data.status_pernikahan, 'status_pernikahan'),
          _badge('Provinsi', data.provinsi, 'provinsi'),
          _badge('Kabupaten/Kota', data.kabupaten, 'kabupaten'),
          _badge('Alamat Lengkap', data.alamat, 'alamat'),
          _badge('Tindakan Medis', data.tindakan, 'tindakan')
        ].join('');
        document.getElementById('p_gpa').innerHTML = [
          _badge('Gravida (G)', data.gpa_gravida, 'gpa_gravida'),
          _badge('Para (P)', data.gpa_para, 'gpa_para'),
          _badge('Abortus (A)', data.gpa_abortus, 'gpa_abortus')
        ].join('');
        const when = (data.updated_at || data.created_at || '').toString().replace('T',' ').slice(0,19);
        if (_editMode) {
          document.getElementById('p_footer').innerHTML = _badge('Tenaga Kesehatan', data.tenaga_kesehatan, 'tenaga_kesehatan');
        } else {
          document.getElementById('p_footer').innerHTML = [
            _badge('Tenaga Kesehatan', data.tenaga_kesehatan),
            _badge('Data Dibuat', when)
          ].join('');
        }
      }

      async function openPatientModal(partnerId){
        if(!partnerId){ return; }
        _editMode = false;
        _patientModal.style.display = 'flex';
        document.getElementById('p_name').textContent = 'Memuat...';
        document.getElementById('p_code').textContent = 'Kode Protokol: -';
        document.getElementById('p_grid').innerHTML = '';
        document.getElementById('p_gpa').innerHTML = '';
        document.getElementById('p_footer').innerHTML = '';
        document.getElementById('p_actions').style.display = 'none';
        document.getElementById('patientEdit').style.display = 'none';
        
        try {
          const res = await fetch(`/api/patient/partner/${partnerId}`);
          const data = await res.json();
          if (!data){
            document.getElementById('p_name').textContent = 'Belum ada data pasien';
            document.getElementById('p_footer').innerHTML = `<a href="/scanner" class="btn-primary" style="text-decoration:none;">üì± Isi Data via Scanner</a>`;
            return;
          }
          
          document.getElementById('patientEdit').style.display = 'flex';
          renderPatientData(data);
        } catch (e){
          document.getElementById('p_name').textContent = 'Gagal memuat data';
        }
      }
      
      // Edit button handler
      document.addEventListener('click', (e) => {
        if (e.target && (e.target.id === 'patientEdit' || e.target.parentElement?.id === 'patientEdit')) {
          toggleEditMode();
        }
      });
      
      // Save button handler
      document.addEventListener('click', async (e) => {
        if (e.target && e.target.id === 'patientSave') {
          if (!_currentPatientData || !_currentPatientData.id) {
            showToast('Data pasien tidak ditemukan', 'error');
            return;
          }
          
          const inputs = document.querySelectorAll('.patient-badge__input');
          const updateData = {};
          inputs.forEach(input => {
            const field = input.dataset.field;
            if (field) {
              updateData[field] = input.value;
            }
          });
          
          try {
            const res = await fetch(`/api/patient/${_currentPatientData.id}`, {
              method: 'PUT',
              headers: { 'Content-Type': 'application/json' },
              body: JSON.stringify(updateData)
            });
            
            const result = await res.json();
            if (res.ok) {
              showToast('Data pasien berhasil diperbarui!', 'success');
              _editMode = false;
              _currentPatientData = { ..._currentPatientData, ...updateData };
              renderPatientData(_currentPatientData);
              
              setTimeout(() => {
                window.location.reload();
              }, 1500);
            } else {
              showToast(result.error || 'Gagal memperbarui data', 'error');
            }
          } catch (error) {
            console.error('Error updating patient:', error);
            showToast('Terjadi kesalahan saat memperbarui data', 'error');
          }
        }
      });
      
      // Cancel button handler
      document.addEventListener('click', (e) => {
        if (e.target && e.target.id === 'patientCancel') {
          _editMode = false;
          if (_currentPatientData) {
            renderPatientData(_currentPatientData);
          }
        }
      });
      // Close with Escape
      document.addEventListener('keydown', (e) => {
        if (e.key === 'Escape') closePatientModal();
      });

      const _periodSelect = document.getElementById('periodSelect');
      if (_periodSelect) _periodSelect.addEventListener('change', function() {
        const customDateRange = document.querySelector('.custom-date-range');
        if (this.value === 'custom') {
          customDateRange.style.display = 'block';
        } else {
          customDateRange.style.display = 'none';
        }
      });

      const _provinceSelect = document.getElementById('provinceSelect');
      if (_provinceSelect) _provinceSelect.addEventListener('change', function() {
        const provinceCode = this.value;
        const partnerSelect = document.getElementById('partnerSelect');
        const addPartnerSection = document.getElementById('addPartnerSection');
        const createBtn = document.getElementById('createProtocolBtn');
        
        if (!provinceCode) {
          partnerSelect.disabled = true;
          partnerSelect.innerHTML = '<option value="">Pilih Provinsi Terlebih Dahulu</option>';
          addPartnerSection.style.display = 'none';
          createBtn.disabled = true;
          return;
        }
        
        fetch(`/api/partners/${provinceCode}`)
          .then(response => response.json())
          .then(partners => {
            partnerSelect.innerHTML = '<option value="">Pilih Mitra</option>';
            
            partners.forEach(partner => {
              const option = document.createElement('option');
              option.value = partner.id;
              option.textContent = `${partner.name} (${partner.code}) - ${partner.type.replace('_', ' ').toUpperCase()}`;
              partnerSelect.appendChild(option);
            });
            
            partnerSelect.disabled = false;
            addPartnerSection.style.display = 'block';
          })
          .catch(error => {
            console.error('Error:', error);
            partnerSelect.innerHTML = '<option value="">Error memuat mitra</option>';
          });
      });
      
      const _partnerSelect = document.getElementById('partnerSelect');
      if (_partnerSelect) _partnerSelect.addEventListener('change', function() {
        const createBtn = document.getElementById('createProtocolBtn');
        if (createBtn) createBtn.disabled = !this.value;
      });
      
      const _addPartnerBtn = document.getElementById('addPartnerBtn');
      if (_addPartnerBtn) _addPartnerBtn.addEventListener('click', function() {
        const newForm = document.getElementById('newPartnerForm');
        if (newForm) newForm.style.display = 'block';
        this.style.display = 'none';
      });
      
      const _cancelPartnerBtn = document.getElementById('cancelPartnerBtn');
      if (_cancelPartnerBtn) _cancelPartnerBtn.addEventListener('click', function() {
        const newForm = document.getElementById('newPartnerForm');
        if (newForm) newForm.style.display = 'none';
        const addBtn = document.getElementById('addPartnerBtn');
        if (addBtn) addBtn.style.display = 'block';
        const fields = ['partner_name','partner_type','partner_code','partner_phone','partner_address'];
        fields.forEach(id => { const el = document.getElementById(id); if (el) el.value = ''; });
      });
      
      const _savePartnerBtn = document.getElementById('savePartnerBtn');
      if (_savePartnerBtn) _savePartnerBtn.addEventListener('click', function() {
        const provinceCode = document.getElementById('provinceSelect').value;
        const partnerData = {
          name: document.getElementById('partner_name').value,
          type: document.getElementById('partner_type').value,
          code: document.getElementById('partner_code').value,
          province_code: provinceCode,
          phone: document.getElementById('partner_phone').value,
          address: document.getElementById('partner_address').value
        };
        
        if (!partnerData.name || !partnerData.type || !partnerData.code) {
          alert('Mohon isi semua kolom wajib (Nama, Tipe, Kode)');
          return;
        }
        
        fetch('/api/partners', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify(partnerData)
        })
        .then(response => {
          if (response.ok) {
            return response.json();
          }
          return response.json().then(err => Promise.reject(err));
        })
        .then(data => {
          const partnerSelect = document.getElementById('partnerSelect');
          const option = document.createElement('option');
          option.value = data.partner.id;
          option.textContent = `${data.partner.name} (${data.partner.code}) - ${data.partner.type.replace('_', ' ').toUpperCase()}`;
          option.selected = true;
          partnerSelect.appendChild(option);
          
          document.getElementById('createProtocolBtn').disabled = false;
          document.getElementById('cancelPartnerBtn').click();
          
          setTimeout(() => {
            window.location.reload();
          }, 1500);
          
          showToast('Mitra berhasil dibuat!', 'success');
        })
        .catch(error => {
          console.error('Error:', error);
          showToast(error.error || 'Error membuat mitra. Silakan coba lagi.', 'error');
        });
      });

      document.addEventListener('DOMContentLoaded', function() {
        initializeCharts();
      });

      function initializeCharts() {
        // Get current theme
        const currentTheme = document.documentElement.getAttribute('data-theme') || 'light';
        const isDark = currentTheme === 'dark';
        
        // OBGYN Theme-aware colors
        const themeColors = {
          text: isDark ? '#F3E8F7' : '#2D1B3D',
          grid: isDark ? 'rgba(200, 181, 214, 0.15)' : 'rgba(155, 126, 189, 0.15)',
          border: isDark ? 'rgba(200, 181, 214, 0.2)' : '#E1D5E7',
          tooltipBg: isDark ? 'rgba(45, 27, 61, 0.95)' : 'rgba(255, 255, 255, 0.95)',
          chartBg: isDark ? '#2D1B3D' : '#FFFFFF'
        };

        const commonOptions = {
          responsive: true,
          maintainAspectRatio: true,
          plugins: {
            legend: {
              labels: {
                color: themeColors.text,
                font: { size: 12, weight: '500' },
                padding: 15
              }
            },
            tooltip: {
              backgroundColor: themeColors.tooltipBg,
              titleColor: themeColors.text,
              bodyColor: themeColors.text,
              borderColor: '#D63384',
              borderWidth: 1,
              padding: 12,
              cornerRadius: 8
            }
          },
          scales: {
            x: {
              ticks: { color: themeColors.text, font: { size: 11 } },
              grid: { color: themeColors.grid, drawBorder: false }
            },
            y: {
              ticks: { color: themeColors.text, font: { size: 11 } },
              grid: { color: themeColors.grid, drawBorder: false }
            }
          }
        };

        const dailyTrendsCtx = document.getElementById('dailyTrendsChart').getContext('2d');
        const dailyTrendsData = <%- JSON.stringify(analytics.dailyTrends) %>;
        
        new Chart(dailyTrendsCtx, {
          type: 'line',
          data: {
            labels: dailyTrendsData.map(d => {
              const date = new Date(d.date);
              return `${date.getDate()} ${date.toLocaleDateString('id-ID', { month: 'short' })}`;
            }),
            datasets: [{
              label: 'Dibuat',
              data: dailyTrendsData.map(d => d.created),
              borderColor: '#6366F1',
              backgroundColor: 'rgba(99, 102, 241, 0.15)',
              borderWidth: 2.5,
              tension: 0.4,
              pointRadius: 4,
              pointBackgroundColor: '#6366F1',
              pointBorderColor: themeColors.chartBg,
              pointBorderWidth: 2
            }, {
              label: 'Terkirim',
              data: dailyTrendsData.map(d => d.delivered),
              borderColor: '#F59E0B',
              backgroundColor: 'rgba(245, 158, 11, 0.15)',
              borderWidth: 2.5,
              tension: 0.4,
              pointRadius: 4,
              pointBackgroundColor: '#F59E0B',
              pointBorderColor: themeColors.chartBg,
              pointBorderWidth: 2
            }, {
              label: 'Terpakai',
              data: dailyTrendsData.map(d => d.terpakai),
              borderColor: '#10B981',
              backgroundColor: 'rgba(16, 185, 129, 0.15)',
              borderWidth: 2.5,
              tension: 0.4,
              pointRadius: 4,
              pointBackgroundColor: '#10B981',
              pointBorderColor: themeColors.chartBg,
              pointBorderWidth: 2
            }]
          },
          options: {
            ...commonOptions,
            plugins: {
              ...commonOptions.plugins,
              legend: { ...commonOptions.plugins.legend, position: 'top' }
            }
          }
        });

        const hourlyCtx = document.getElementById('hourlyChart').getContext('2d');
        const hourlyData = <%- JSON.stringify(analytics.hourlyDistribution) %>;
        
        new Chart(hourlyCtx, {
          type: 'bar',
          data: {
            labels: Array.from({length: 24}, (_, i) => `${i}:00`),
            datasets: [{
              label: 'Protokol Dibuat',
              data: Array.from({length: 24}, (_, i) => {
                const hour = hourlyData.find(h => parseInt(h.hour) === i);
                return hour ? hour.count : 0;
              }),
              backgroundColor: 'rgba(214, 51, 132, 0.25)',
              borderColor: '#D63384',
              borderWidth: 1.5
            }]
          },
          options: {
            ...commonOptions,
            plugins: {
              ...commonOptions.plugins,
              legend: { display: false }
            }
          }
        });

        const statusPieCtx = document.getElementById('statusPieChart').getContext('2d');
        
        new Chart(statusPieCtx, {
          type: 'doughnut',
          data: {
            labels: ['Dibuat', 'Terkirim', 'Terpakai'],
            datasets: [{
              data: [<%= stats.created %>, <%= stats.delivered %>, <%= stats.terpakai %>],
              backgroundColor: ['#6366F1', '#F59E0B', '#10B981'],
              borderWidth: 3,
              borderColor: themeColors.chartBg
            }]
          },
          options: {
            responsive: true,
            plugins: {
              legend: {
                position: 'bottom',
                labels: {
                  color: themeColors.text,
                  font: { size: 12, weight: '500' },
                  padding: 15
                }
              },
              tooltip: {
                backgroundColor: themeColors.tooltipBg,
                titleColor: themeColors.text,
                bodyColor: themeColors.text,
                borderColor: '#D63384',
                borderWidth: 1,
                padding: 12,
                cornerRadius: 8
              }
            }
          }
        });

        const provinceCtx = document.getElementById('provinceChart').getContext('2d');
        const provinceData = <%- JSON.stringify(analytics.provincePerformance || []) %>;

        new Chart(provinceCtx, {
          type: 'bar',
          data: {
            labels: provinceData.map(p => p.province_code),
            datasets: [{
              label: 'Jumlah Protokol',
              data: provinceData.map(p => p.count),
              backgroundColor: ['#D63384', '#9B7EBD', '#E685B5', '#6366F1', '#F59E0B'],
              borderWidth: 0
            }]
          },
          options: {
            ...commonOptions,
            indexAxis: 'y',
            plugins: {
              ...commonOptions.plugins,
              legend: { display: false }
            },
            scales: {
              x: {
                ticks: { color: themeColors.text, font: { size: 11 } },
                grid: { color: themeColors.grid, drawBorder: false },
                beginAtZero: true
              },
              y: {
                ticks: { color: themeColors.text, font: { size: 11 } },
                grid: { display: false }
              }
            }
          }
        });
      }

      // Reinitialize charts when theme changes
      window.addEventListener('themechange', function() {
        // Destroy existing charts
        Chart.helpers.each(Chart.instances, function(instance) {
          instance.destroy();
        });
        // Reinitialize with new theme
        initializeCharts();
      });

      /* setupPartnerDropdown removed; see /mitra dashboard */

      // Mobile Navigation with Smooth Transitions
      const mobileMenuToggle = document.getElementById('mobileMenuToggle');
      const mobileNav = document.getElementById('mobileNav');
      const mobileNavBackdrop = document.getElementById('mobileNavBackdrop');

      if (mobileMenuToggle) {
        mobileMenuToggle.addEventListener('click', function() {
          this.classList.toggle('active');
          mobileNav.classList.toggle('active');
          mobileNavBackdrop.classList.toggle('active');
          document.body.style.overflow = mobileNav.classList.contains('active') ? 'hidden' : '';
        });

        mobileNavBackdrop.addEventListener('click', function() {
          mobileMenuToggle.classList.remove('active');
          mobileNav.classList.remove('active');
          this.classList.remove('active');
          document.body.style.overflow = '';
        });

        const navLinks = mobileNav.querySelectorAll('a');
        navLinks.forEach(link => {
          link.addEventListener('click', function() {
            mobileMenuToggle.classList.remove('active');
            mobileNav.classList.remove('active');
            mobileNavBackdrop.classList.remove('active');
            document.body.style.overflow = '';
          });
        });
      }

      // Enhanced Mobile UX: Smooth Scroll & Page Transitions
      document.addEventListener('DOMContentLoaded', function() {
        // Add page transition class
        document.body.classList.add('page-transition');

        // Intersection Observer for section animations
        if ('IntersectionObserver' in window) {
          const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
              if (entry.isIntersecting) {
                entry.target.style.opacity = '1';
                entry.target.style.transform = 'translateY(0)';
              }
            });
          }, {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
          });

          document.querySelectorAll('section').forEach(section => {
            section.style.opacity = '0';
            section.style.transform = 'translateY(20px)';
            section.style.transition = 'opacity 0.6s ease-out, transform 0.6s ease-out';
            observer.observe(section);
          });
        }

        // Haptic feedback for button clicks (if supported)
        document.querySelectorAll('button, .btn').forEach(button => {
          button.addEventListener('click', function() {
            if (navigator.vibrate) {
              navigator.vibrate(10); // Very light haptic feedback
            }
          });
        });

        // Add ripple effect to buttons
        document.querySelectorAll('button, .btn').forEach(button => {
          button.addEventListener('click', function(e) {
            const ripple = document.createElement('span');
            const rect = this.getBoundingClientRect();
            const size = Math.max(rect.width, rect.height);
            const x = e.clientX - rect.left - size / 2;
            const y = e.clientY - rect.top - size / 2;
            
            ripple.style.cssText = `
              position: absolute;
              width: ${size}px;
              height: ${size}px;
              left: ${x}px;
              top: ${y}px;
              border-radius: 50%;
              background: rgba(255, 255, 255, 0.5);
              pointer-events: none;
              animation: rippleEffect 0.6s ease-out;
            `;
            
            this.style.position = 'relative';
            this.style.overflow = 'hidden';
            this.appendChild(ripple);
            
            setTimeout(() => ripple.remove(), 600);
          });
        });

        // Add CSS for ripple animation
        const style = document.createElement('style');
        style.textContent = `
          @keyframes rippleEffect {
            from {
              transform: scale(0);
              opacity: 1;
            }
            to {
              transform: scale(2);
              opacity: 0;
            }
          }
        `;
        document.head.appendChild(style);
      });

      // Smooth scroll to top on page refresh
      if ('scrollRestoration' in history) {
        history.scrollRestoration = 'manual';
      }
      window.scrollTo(0, 0);
    </script>
  </body>
</html>